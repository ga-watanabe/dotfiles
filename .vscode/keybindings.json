[
  { "key": "ctrl+a", "command": "macros.saveAndFocusTerminal", "when": "editorTextFocus" }, // saveAndFocusTerminalはsetting.jsonに定義してある
  { "key": "ctrl+a", "command": "workbench.action.focusFirstEditorGroup", "when": "terminalFocus" },
  { "key": "ctrl+]", "command": "workbench.action.nextEditor" },
  { "key": "ctrl+[", "command": "workbench.action.previousEditor" },
  { "key": "shift+alt+up", "command": "editor.action.transformToUppercase" },
  { "key": "shift+alt+down", "command": "editor.action.transformToLowercase" },
  { "key": "ctrl+cmd+c", "command": "copyRelativeFilePath", "when": "editorFocus" }
]
